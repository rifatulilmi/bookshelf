{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [ "airbnb-base"],
    
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-console": "off",
        "indent": [2, "tab"],
        "no-tabs": 0,
        "linebreak-style": [
            "error",
            "windows"
        ],
       
        "semi": [
            "error",
            "always"
        ]
    }
}
